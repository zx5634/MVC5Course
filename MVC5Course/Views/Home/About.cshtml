@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

@functions
{
    public string GetModelStateValue(string name)
    {
        if (ViewData.ModelState[name] != null)
            return ViewData.ModelState[name].Value.AttemptedValue;
        else
            return "";
    }
}

@GetModelStateValue("id")
